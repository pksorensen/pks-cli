{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "pks-cli/src/**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "includePrivateMembers": false,
      "enumSortOrder": "alphabetic",
      "allowCompilationErrors": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**/*.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appName": "PKS CLI",
      "_appTitle": "PKS CLI - Professional Kubernetes Simplifier",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_appFooter": "Copyright © 2024 Poul Kjeldager Sørensen. Built with DocFX.",
      "_gitContribute": {
        "repo": "https://github.com/pksorensen/pks-cli",
        "branch": "main"
      }
    },
    "sitemap": {
      "baseUrl": "https://pksorensen.github.io/pks-cli"
    }
  }
}